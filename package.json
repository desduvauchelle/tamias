{
	"name": "tamias",
	"version": "1.0.14",
	"module": "src/index.ts",
	"type": "module",
	"bin": {
		"tamias": "./src/index.ts"
	},
	"scripts": {
		"start": "bun run src/index.ts start",
		"stop": "bun run src/index.ts stop",
		"status": "bun run src/index.ts status",
		"usage": "bun run src/index.ts usage",
		"onboarding": "bun run src/index.ts onboarding",
		"models": "bun run src/index.ts models",
		"chat": "bun run src/index.ts chat",
		"cron": "bun run src/index.ts cron",
		"build": "bun build --compile --outfile=tamias src/index.ts",
		"install-it": "chmod +x install.sh && ./install.sh",
		"test": "bun test",
		"type-check": "bunx tsc --noEmit"
	},
	"devDependencies": {
		"@types/bun": "latest",
