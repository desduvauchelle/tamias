{
	"name": "tamias",
	"version": "26.2.26.11",
	"module": "src/index.ts",
	"type": "module",
	"bin": {
		"tamias": "./src/index.ts"
	},
	"scripts": {
		"start": "bun run src/index.ts start",
		"stop": "bun run src/index.ts stop",
		"status": "bun run src/index.ts status",
		"usage": "bun run src/index.ts usage",
		"onboarding": "bun run src/index.ts onboarding",
		"models": "bun run src/index.ts models",
		"model": "bun run src/index.ts model",
		"chat": "bun run src/index.ts chat",
		"cron": "bun run src/index.ts cron",
		"build": "bun build --compile --external playwright --outfile=tamias src/index.ts",
		"dev": "bun --hot src/index.ts",
		"install-it": "chmod +x install.sh && ./install.sh",
		"test": "bun test --preload ./src/tests/setup.ts src/tests/*.test.ts src/utils/*.test.ts",
		"test:integration": "./test/integration/run.sh",
		"typecheck": "bunx tsc --noEmit",
		"typecheck:all": "bunx tsc --noEmit && bunx tsc --noEmit -p src/dashboard/tsconfig.json",
		"prepare": "husky",
		"changelog": "bun run src/utils/changelog.ts",
		"bump-version": "bun run src/utils/bump-version.ts"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"@types/fluent-ffmpeg": "^2.1.28",
		"husky": "^9.1.7"
	},
	"peerDependencies": {
		"typescript": "^5.0.0"
	},
	"dependencies": {
		"@ai-sdk/anthropic": "^3.0.46",
		"@ai-sdk/google": "^3.0.30",
		"@ai-sdk/openai": "^3.0.30",
		"@clack/prompts": "^1.0.1",
		"@modelcontextprotocol/sdk": "^1.26.0",
		"@openrouter/ai-sdk-provider": "^2.2.3",
		"@types/marked": "^6.0.0",
		"@xenova/transformers": "^2.17.2",
		"ai": "^6.0.94",
		"commander": "^14.0.3",
		"croner": "^10.0.1",
		"discord.js": "^14.25.1",
		"ffmpeg-static": "^5.3.0",
		"fluent-ffmpeg": "^2.1.3",
		"grammy": "^1.40.0",
		"gray-matter": "^4.0.3",
		"marked": "^17.0.3",
		"marked-terminal": "^7.0.0",
		"picocolors": "^1.1.1",
		"playwright": "^1.58.2",
		"wavefile": "^11.0.0",
		"zod": "^4.3.6"
	}
}
